<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link rel="stylesheet" type="text/css" href="/stylesheets/searchProceedings.css">
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
<link rel="stylesheet" type="text/css" href="/stylesheets/cloud.css">
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="/js/searchProceedings.js"></script>
<!------ Include the above in your HEAD tag ---------->

<div class="container">
	<div class="row">
		<section class="content">
			<h1>Pesquisa por Trabalhos</h1>
			<div class="col-md-8 col-md-offset-2">
				<div class="panel panel-default">
						<div class="table-container">
							<table class="table table-filter">
								<tbody>
										<h1>Professores</h1>
										<center><h5>Clique para ver as áreas de conhecimento baseadas nas publicações</h5></center>
										<div class="cloud">
											<%- cloud %>
										</div>
								</tbody>
							</table>
						</div>

						<div class="panel-body">
								<div class="pull-right">
									<div class="btn-group">
										<button type="button" class="btn btn-success btn-filter" data-target="Tìtulo">Tìtulo</button>
										<button type="button" class="btn btn-warning btn-filter" data-target="Autor">Autor</button>
										<button type="button" class="btn btn-danger btn-filter" data-target="Aluno">Aluno</button>
										<button type="button" class="btn btn-default btn-filter" data-target="all">Todos</button>
									</div>
								</div>
						<div class="table-container">
							<table class="table table-filter">
								<tbody>
										<h1>Trabalhos Relacionados</h1>
									<% for (proceeding of proceedingsByName){%>
									<tr data-status="Tìtulo">
										<td>
											<div class="ckbox">
												<input type="checkbox" id="checkbox1">
												<label for="checkbox1"></label>
											</div>
										</td>
										<td>
											<a href="javascript:;" class="star">
												<i class="glyphicon glyphicon-star"></i>
											</a>
										</td>
										<td>
												<div class="col-sm-4">
												</div>

												<div class="media">
													<a href="#" class="pull-left">
														<img src="https://s3.amazonaws.com/uifaces/faces/twitter/fffabs/128.jpg" class="media-photo">
													</a>
													<div class="media-body">
														<span class="media-meta pull-right">Febrero 13, 2016</span>
														<h4 class="title">
															<%if (proceeding[3] == 'null') {%>
																<%= proceeding[1] %>
															<%} else {%>
																<a href="<%= proceeding[3] %>" target="blank"> <%= proceeding[1] %> </a>
															<%}%>

															<span class="pull-right Tìtulo">(Tìtulo)</span>
														</h4>
														<p class="summary">Autores:
															<% for (author of proceeding[2]){%>
																<span><%= author %>;</span>
															<%}%>
														</p>
														<p class="summary">Alunos Envolvidos:
															<% for (student of proceeding[4]){%>
																<span><%= student %>;</span>
															<%}%>
														</p>
													</div>
												</div>
										</td>
                                    </tr>
									<% } %>

									<% for (proceeding of proceedingsByAuthor){%>
										<tr data-status="Autor">
											<td>
												<div class="ckbox">
													<input type="checkbox" id="checkbox1">
													<label for="checkbox1"></label>
												</div>
											</td>
											<td>
												<a href="javascript:;" class="star">
													<i class="glyphicon glyphicon-star"></i>
												</a>
											</td>
											<td>
													<div class="col-sm-4">
													</div>
	
													<div class="media">
														<a href="#" class="pull-left">
															<img src="https://s3.amazonaws.com/uifaces/faces/twitter/fffabs/128.jpg" class="media-photo">
														</a>
														<div class="media-body">
															<span class="media-meta pull-right">Febrero 13, 2016</span>
															<h4 class="title">
																<%if (proceeding[3] == 'null') {%>
																	<%= proceeding[1] %>
																<%} else {%>
																	<a href="<%= proceeding[3] %>" target="blank"> <%= proceeding[1] %> </a>
																<%}%>
	
																<span class="pull-right Tìtulo">(Tìtulo)</span>
															</h4>
															<p class="summary">Autores:
																<% for (author of proceeding[2]){%>
																	<span><%= author %>;</span>
																<%}%>
															</p>
															<p class="summary">Alunos Envolvidos:
																<% for (student of proceeding[4]){%>
																	<span><%= student %>;</span>
																<%}%>
															</p>
														</div>
													</div>
											</td>
										</tr>
										<% } %>

										<% for (proceeding of proceedingsByStudents){%>
										<tr data-status="Aluno">
											<td>
												<div class="ckbox">
													<input type="checkbox" id="checkbox1">
													<label for="checkbox1"></label>
												</div>
											</td>
											<td>
												<a href="javascript:;" class="star">
													<i class="glyphicon glyphicon-star"></i>
												</a>
											</td>
											<td>
													<div class="col-sm-4">
													</div>
	
													<div class="media">
														<a href="#" class="pull-left">
															<img src="https://s3.amazonaws.com/uifaces/faces/twitter/fffabs/128.jpg" class="media-photo">
														</a>
														<div class="media-body">
															<span class="media-meta pull-right">Febrero 13, 2016</span>
															<h4 class="title">
																<%if (proceeding[3] == 'null') {%>
																	<%= proceeding[1] %>
																<%} else {%>
																	<a href="<%= proceeding[3] %>" target="blank"> <%= proceeding[1] %> </a>
																<%}%>
	
																<span class="pull-right Tìtulo">(Tìtulo)</span>
															</h4>
															<p class="summary">Autores:
																	<% for (author of proceeding[2]){%>
																		<span><%= author %>;</span>
																	<%}%>
															</p>
															<p class="summary">Alunos Envolvidos:
																<% for (student of proceeding[4]){%>
																	<span><%= student %>;</span>
																<%}%>
															</p>
														</div>
													</div>
											</td>
										</tr>
										<% } %>
								</tbody>
							</table>
						</div>
					</div>
				</div>
				<div class="content-footer">
					<p>
						Page © - 2016 <br>
						Powered By <a href="https://www.facebook.com/tavo.qiqe.lucero" target="_blank">TavoQiqe</a>
					</p>
				</div>
			</div>
		</section>
		
	</div>
</div>